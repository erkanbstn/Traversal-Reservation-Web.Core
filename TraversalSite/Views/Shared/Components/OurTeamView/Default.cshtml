@model List<Guide>

<section class="w3l-team" id="team">
    <div class="team-block py-5">
        <div class="container py-lg-5">
            <div class="title-content text-center mb-lg-3 mb-4">
                <h6 class="sub-title">Ekibimiz</h6>
                <h3 class="hny-title">Rehberlerimizle Tanışın</h3>
            </div>
            <div class="row">


                @foreach (var guid in Model)
                {
                    <div class="col-lg-3 col-6 mt-lg-5 mt-4">

                        <div class="box16">
                            <a href="#url"><img src="~/Tra/assets/images/@guid.Image" alt="" class="img-fluid" /></a>
                            <div class="box-content">
                                <h3 class="title"><a href="#url">@guid.Name</a></h3>
                                <span class="post">@guid.Description</span>
                                <ul class="social">
                                    <li>
                                        <a href="@guid.InstagramUrl" target="_blank" class="linkedin">
                                            <span class="fa fa-linkedin"></span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>

                }

                </>
            </div>
        </div>
</section>
